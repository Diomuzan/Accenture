{% extends "Accenture_Base.html.twig" %}


{% block body %}
<h1 class="heading1 position-relative text-light">Accenture</h1>

    <table class="accentureTable table text-light position-relative">
        <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Class</th>
        </tr>
        </thead>
        <tbody>
            {% for data in data  %}
            <tr>
            <td>{{ data.id }}</td>
            <td>{{ data.name }}</td>
            <td>{{ data.class }}</td>
        </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}

